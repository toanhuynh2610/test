<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magenest_Movie::movie_menu" title="Movie">
                    <resource id="Magenest_Movie::index" title="Index">
                        <resource id="Magenest_Movie::movie_submenu" title="Movie"></resource>
                        <resource id="Magenest_Movie::actor" title="Actor"></resource>
                        <resource id="Magenest_Movie::director" title="Director"></resource>
                    </resource>
                    <resource id="Magenest_Movie::section" title="Section">
                        <resource id="Magenest_Movie::number" title="Number"></resource>
                    </resource>
                </resource>
                    <resource id="Magento_Backend::stores">
                        <resource id="Magento_Backend::stores_settings">
                            <resource id="Magento_Config::config">
                                <resource id="Magenest_Movie::config_movie" title="Movie" />
                            </resource>
                        </resource>
                    </resource>
            </resource>
        </resources>
    </acl>
</config>